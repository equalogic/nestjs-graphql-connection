{
  "name": "@madsci/graphql-pagination",
  "version": "0.1.2",
  "description": "Relay-style pagination for GraphQL using Connections, Edges and Cursors.",
  "author": "Madscience Ltd",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madscience/graphql-pagination.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "postversion": "cp package.json README.md dist/",
    "generate-barrels": "barrelsby --config barrelsby.json",
    "release": "npx np --no-publish --contents dist/"
  },
  "dependencies": {
    "graphql-relay": "^0.6.0",
    "joi": "^14.3.1",
    "type-graphql": "^0.17.6"
  },
  "devDependencies": {
    "@types/graphql-relay": "^0.4.11",
    "@types/jest": "^25.1.3",
    "@types/joi": "^14.3.4",
    "@types/node": "13.x",
    "@typescript-eslint/eslint-plugin": "=2.26.0",
    "@typescript-eslint/parser": "^2.22.0",
    "barrelsby": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "main": "index.js",
  "np": {
    "contents": "dist",
    "publish": false,
    "yarn": false,
    "cleanup": true
  }
}
